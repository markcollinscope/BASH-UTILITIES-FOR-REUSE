. utils.shi

fn()
{
	echo hello-world;
}
fn
declare -f fn # prints above.

fn()
{
	declare -f fn;
}
fn # prints latest fn.
declare -f fn # prints as per last line.

echo CLONE:
clonefn()
{
	local new=$1;
	local old=$2;

	local backup=$(declare -f $old | grep -v $old);	

	eval "
		$new() $backup;
	"

}
afn() { echo aaaaa; }
afn

clonefn bfn afn;
bfn

echo EXTEND:
fnnm() { local xt=__ ; echo $1$xt; }
extendfn()
{
	if test "$1" = "--nm"; then echo $fnnm $2; return 0; fi

	local fn=$1;
	# local extendingfn=$2;
	local newfn=$(fnnm $fn);

	clonefn $newfn $fn
}

extendfn afn
newfn=$(extendfn --nm afn);
$newfn


# Objective 
: extendfn fnname "{ body? }" or extendfn fnname newfn?
: extendfn


#!/bin/bash

# auto parse direct request for Usage information.

. utils_globals.shi

if ! alreadyIncluded UTILS_HELP; then 
. utils_core.shi

if ! $UTS_NOAUTOARGS; then 
	if test "$1" = "--help"; then shift; callFnIfExists Usage; exitok; fi
	if test "$1" = "-?"; then shift; callFnIfExists Usage; exitok; fi
fi

fi
